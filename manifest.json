{
  "manifest_version": 3,
  "name": "Let's Level up",
  "version": "1.0",
  "permissions": ["sidePanel","storage", "tabs", "notifications"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": "images/icon.png",
    "default_popup": "bin/main/main.html",
    "default_title": "Open Main Popup"
  },
  "side_panel": {
    "default_path": "bin/sidepanel/sidepanel.html"
  }
}