{
  "manifest_version": 3,
  "name": "Let's Level up",
  "version": "1.0",
  "permissions": ["sidePanel"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "main.html",
    "default_title": "Open Main Popup"
  },
  "side_panel": {
    "default_path": "bin/sidepanel/sidepanel.html"
  }
}